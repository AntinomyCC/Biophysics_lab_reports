	pages = {3784--3788},
	annote = {Gasteiger E, Gattiker A, Hoogland C, Ivanyi I, Appel RD, Bairoch A. ExPASy: The proteomics server for in-depth protein knowledge and analysis. Nucleic Acids Res. 2003 Jul 1;31(13):3784-8. doi: 10.1093/nar/gkg563. PMID: 12824418; PMCID: PMC168970.},
	file = {Full Text:C\:\\Users\\Dahoam\\Zotero\\storage\\IWJQRUDP\\Gasteiger et al. - 2003 - ExPASy The proteomics server for in-depth protein.pdf:application/pdf},

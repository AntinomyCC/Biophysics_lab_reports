	file = {Full Text PDF:C\:\\Users\\Dahoam\\Zotero\\storage\\TCP9JXDM\\Chang et al. - 2021 - BRENDA, the ELIXIR core data resource in 2021 new.pdf:application/pdf;Snapshot:C\:\\Users\\Dahoam\\Zotero\\storage\\UID4TLAL\\5992283.html:text/html},
